function handleData(data){	var p = data.split("%");	var raw = data.split("%");	raw.shift();	switch (p[1].toLowerCase())	{		case "err" :			showError(raw[1]);			break;		case "cnu" :			WORLD.playerPush[raw[1]] = {raw:raw};			break;		case "ap" :			handleBuildPlayer(raw);			break;		case "wrl" :			gotoAndStop("chat");			WORLD.ROOM = room_mc;			applyTriggers();			WORLD.PLAYERS = room_mc.players_mc;			Sys.myId = raw[1];			break;		case "pos" :			var x = raw[1];			var y = raw[2];			var id = raw[3];			movePlayer(id,x,y);			break;		case "rm" :			var id = raw[1];			Sys.myRoomId = id;			handleLoadRoom(id);			WORLD.ROOM.id = id;			break;		case "al":			var alerts = raw[1];			showAlerts(100);			break;		case "sf" :			var id = raw[1];			var myId = raw[2];			handleSetFrame(myId,id,true);			break;		case "sm" :			var message = raw[1];			var id = raw[2];			handleSendMessage(id,message);			break;		case "sp" :			var x = raw[1];			var y = raw[2];			var id = raw[3];			movePlayer(id,x,y);			break;		case "se" :			var emote = raw[1];			var id = raw[2];			handleSendEmote(id,emote);			break;		case "upi" :			var type = raw[1];			var value = raw[2];			var id = raw[3];			handleUpdateItem(type,value,id);			break;		case "ai":			var id = raw[1];			handleAddItem(id);			break;		case "ri":			var id = raw[1];			var inventory = raw[2];			handleRemoveItem(id, inventory);		break;		case "ma":					break;		case "rcv":					break;		case "rp":			var id = raw[1];			handleRemovePlayer(id);			break;	}}