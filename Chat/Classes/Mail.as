class Classes.Mail {	private var sender = 0;	private var message = "";	private var cleared = false;		public function setSender(id:Number){		this.sender = id;	}	public function getSender() {		return this.sender;	}	public function setCleared(state:Boolean) {		this.cleared = state;	}	public function getCleared() {		return this.cleared;	}	public function setMessage(message:String) {		this.message = message;	}	public function getMessage() {		return this.message;	}}