class Classes.Client{	var id;	var name;	var color;	var head;	var body;	var face;	var neck;	var feet;	var hand;	var photo;	var flag;	var age;	var rank;	var mood;	var coins = 0;	var mc;	var alerts = 0;	var crumbs;	var obj;	function Client(playerObj, mc, alerts)	{		var data = playerObj.split("|");		var obj = new Object();		obj.id = data[0];		obj.name = data[1];		obj.color = data[2];		obj.head = data[3];		obj.body = data[4];		obj.face = data[5];		obj.neck = data[6];		obj.feet = data[7];		obj.hand = data[8];		obj.age = data[9];		obj.rank = data[10];		obj.mood = data[11];		obj.coins = data[12];		obj.inventory = data[13];		obj.flag = data[14];		obj.photo = data[15];		this.obj = obj;		this.crumbs = playerObj;		this.id = obj.id;		this.name = obj.name;		this.color = obj.color;		this.head = obj.head;		this.body = obj.body;		this.face = obj.face;		this.neck = obj.neck;		this.feet = obj.neck;		this.hand = obj.hand;		this.age = obj.age;		this.rank = obj.rank;		this.mood = obj.mood;		this.coins = obj.coins;		this.flag = obj.flag;		this.photo = obj.photo;		this.alerts = alerts;		this.mc = mc;		this.setupMovie();		_level0.showAlerts(this.alerts);	}	function setupMovie(){		this.mc.head.loadMovie(_level0.getMedia('clothing/sprites', this.head));		this.mc.body.loadMovie(_level0.getMedia('clothing/sprites', this.body));		this.mc.face.loadMovie(_level0.getMedia('clothing/sprites', this.face));		this.mc.neck.loadMovie(_level0.getMedia('clothing/sprites', this.neck));		this.mc.feet.loadMovie(_level0.getMedia('clothing/sprites', this.feet));		this.mc.hand.loadMovie(_level0.getMedia('clothing/sprites', this.hand));	}}