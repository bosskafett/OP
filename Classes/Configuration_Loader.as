class Classes.Configuration_Loader {	private var configuration_array:Array;		public function config_get(data:String)	{		if(this.configuration_array[data] == undefined){			trace("Invalid configuration value.");			return;		}		return this.configuration_array[data];	}	public function config_set(data:String, value)	{		if(!data)			return 0;		this.configuration_array[data] = value;		return 1;	}	public function set_data(array:Array)	{		if(this.configuration_array == undefined)			this.configuration_array = array;		else			return 0;		return 1;	}}